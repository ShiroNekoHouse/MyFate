{
    "local": {
        "role": "host",
        "party_id": _host_party_id
    },
    "role": {
        "host": [
            _host_party_id
        ],
        "guest": [
            _guest_party_id
        ]
    },
    "DataIOParam": {
        "input_format": "dense",
        "delimitor": ",",
        "with_label": false,
        "label_idx": 0,
        "output_format": "sparse",
        "missing_fill": true,
        "outlier_replace": true
    },
    "WorkFlowParam": {
        "method": "_workflow_method",
        "train_input_table": "_train_table_name",
        "train_input_namespace": "example_data_namespace",
        "model_table": "boostingtree_model_host",
        "model_namespace": "boostingtree_model_namespace",
        "predict_input_table": "_predict_table_name",
        "predict_input_namespace": "example_data_namespace",
        "predict_result_partition": 10,
        "predict_output_table": "host_predict_output_table__jobid",
        "predict_output_namespace": "host_predict_output_path",
        "evaluation_output_table": "evaluate_output_table__jobid",
        "evaluation_output_namespace": "host_evaluate_output_path",
        "data_input_table": "_cross_validation_table_name",
        "data_input_namespace": "example_data_namespace",
        "n_splits": 5,
        "work_mode": _work_mode,
        "need_sample": true,
        "need_feature_selection": false
    },
    "EvaluateParam":{
        "metrics": ["auc", "precision"],
        "classi_type": "binary",
        "pos_label": 1,
        "predict_threshold": [0.5]
    },
    "SampleParam": { 
        "mode": "random",
        "method": "downsample"
    },
    "DecisionTreeParam": {
        "criterion_method": "xgboost",
        "criterion_params": [0.1],
        "max_depth": 5,
        "min_sample_split": 2,
        "min_impurity_split": 0.001,
        "min_leaf_noe": 10,
        "max_split_nodes": 1024,
        "n_iter_no_change": true,
        "tol": 0.0001
    },
    "BoostingTreeParam": {
        "loss_type": "cross_entropy",
        "learning_rate": 0.1,
        "num_trees": 2,
        "subsample_feature_rate": 1,
        "n_iter_no_change": false,
        "tol": 0.0001,
        "bin_num": 50,
        "bin_gap": 0.000001,
        "bin_sample_num": 150000
    }
}
