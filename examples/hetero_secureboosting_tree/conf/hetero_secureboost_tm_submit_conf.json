{
    "task": "cross_validation",
    "module": "HeteroSecureBoostingTree",
    "role": {
        "guest": [9999],
        "host": [10000]
    },
    "role_parameters": {
        "guest": {
            "WorkFlowParam": {
                "data_input_table": ["hetero_secure_boost_guest_data"], 
                "data_input_namespace": ["hetero_secure_boost"]
            },
            "DataIOParam":{
                "with_label": [true],
                "label_idx": [0],
                "label_type": ["int"],
                "output_format": ["sparse"]
            },
            "ObjectiveParam": {
                "objective": ["cross_entropy"]
            }
        },
        "host": {
            "WorkFlowParam": {
                "data_input_table": ["hetero_secure_boost_host_data"],
                "data_input_namespace": ["hetero_secure_boost"]
            },
             "DataIOParam":{
                "with_label": [false],
                "output_format": ["sparse"]
            }
        }
    },
    "algorithm_parameters": {
        "WorkFlowParam": {
            "work_mode": 1,
            "n_splits": 5
        },
        "EvaluateParam": {
            "metrics": ["auc"],
            "classi_type": "binary",
            "pos_label": 1,
            "predict_threshold": [0.5]
        },
        "BoostingTreeParam": {
            "task_type": "classification",
            "learning_rate": 0.1,
            "num_trees": 10,
            "subsample_feature_rate": 1,
            "n_iter_no_change": false,  
            "tol": 0.0001,
            "bin_gap": 0.000001,
            "bin_sample_num": 150000
        }
    }
}
